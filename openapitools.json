{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.6.0",
    "generators": {
      "typescript-fetch": {
        "generatorName": "typescript-fetch",
        "output": "./src/typescript-fetch/#{name}",
        "glob": "./hospitality-api-docs/rest-api-specs/property/*.{json,yaml}",
        "skipOperationExample": true,
        "templateDir": "./templates/typescript-fetch"
      },
      "go": {
        "generatorName": "go",
        "output": "./src/go/#{name}",
        "glob": "./hospitality-api-docs/rest-api-specs/property/*.{json,yaml}",
        "gitRepoId": "ohip-sdk",
        "gitUserId": "applyinnovations",
        "templateDir": "./templates/go",
        "skipOperationExample": true,
        "additionalProperties": {
          "isGoSubmodule": true,
          "packageName": "#{name}",
          "structPrefix": true,
          "enumClassPrefix": true,
          "modulePath": "src/go"
        }
      },
      "go-oauth": {
        "generatorName": "go",
        "output": "./src/go/oauth",
        "glob": "./hospitality-api-docs/rest-api-specs/property/oauth.json",
        "gitRepoId": "ohip-sdk",
        "gitUserId": "applyinnovations",
        "templateDir": "./templates/go-auth",
        "skipOperationExample": true,
        "additionalProperties": {
          "isGoSubmodule": true,
          "packageName": "oauth",
          "structPrefix": true,
          "enumClassPrefix": true,
          "modulePath": "src/go"
        }
      }
    }
  }
}
