{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.6.0",
    "generators": {
      "go": {
        "generatorName": "go",
        "output": "src/apis/go/#{name}",
        "glob": "./hospitality-api-docs/rest-api-specs/property/*.{json,yaml}",
        "gitRepoId": "ohip-sdk",
        "gitUserId": "applyinnovations",
        "skipOperationExample": true,
        "additionalProperties": {
          "isGoSubmodule": true,
          "packageName": "#{name}",
          "structPrefix": true,
          "enumClassPrefix": true
        }
      }
    }
  }
}
